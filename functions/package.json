    "dotenv": "^16.3.1",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^7.0.3",
    "jsdom": "^23.0.0",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.1.0"
  },
